<?xml version="1.0" ?>
<robot name="my_rb1_robot">

  <link name="base_footprint"/>

  <!-- Joint/Link - Chassis -->
  <joint name="joint_base_link" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0.15"/>
    <child link="base_link"/>
    <parent link="base_footprint"/>
  </joint>
  <link name="base_link">
    <inertial>
      <mass value="16.5"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.393" ixy="0" ixz="0" iyy="0.393" iyz="0" izz="0.531"/>
    </inertial>
    <collision>
      <geometry>
        <cylinder length="0.3" radius="0.25"/>
      </geometry>
    </collision>
    <visual>
      <geometry>
        <cylinder length="0.3" radius="0.25"/>
      </geometry>
      <material name="Black">
        <color rgba="0.1 0.1 0.1 1"/>
      </material>
    </visual>
  </link>

  <!-- Joint/Link - left wheel -->
  <joint name="joint_left_wheel" type="continuous">
    <origin rpy="0 0 0" xyz="0.0 0.2 -0.15"/>
    <child link="left_wheel"/>
    <parent link="base_link"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>
  <link name="left_wheel">
    <inertial>
      <mass value="2"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.0206" ixy="0" ixz="0" iyy="0.0206" iyz="0" izz="0.04"/>
    </inertial>
    <collision>
      <origin rpy="1.5708 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.02" radius="0.025"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="1.5708 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.02" radius="0.025"/>
      </geometry>
      <material name="Blue">
        <color rgba="0.1 0.1 0.4 1"/>
      </material>
    </visual>
  </link>

  <!-- Joint/Link - right wheel -->
  <joint name="joint_right_wheel" type="continuous">
    <origin rpy="0 0 0" xyz="0.0 -0.2 -0.15"/>
    <child link="right_wheel"/>
    <parent link="base_link"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>
  <link name="right_wheel">
    <inertial>
      <mass value="2"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.0206" ixy="0" ixz="0" iyy="0.0206" iyz="0" izz="0.04"/>
    </inertial>
    <collision>
      <origin rpy="1.5708 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.02" radius="0.025"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="1.5708 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.02" radius="0.025"/>
      </geometry>
      <material name="Blue">
        <color rgba="0.1 0.1 0.4 1"/>
      </material>
    </visual>
  </link>

  <!-- Joint/Link - front caster -->
  <joint name="joint_front_caster" type="fixed">
    <origin rpy="0 0 0" xyz="0.1 0.0 -0.15"/>
    <child link="front_caster"/>
    <parent link="base_link"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="0.0"/>
  </joint>
  <link name="front_caster">
    <inertial>
      <mass value="2"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.00025" ixy="0" ixz="0" iyy="0.00025" iyz="0" izz="0.00025"/>
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.025"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.025"/>
      </geometry>
      <material name="Blue">
        <color rgba="0.1 0.1 0.4 1"/>
      </material>
    </visual>
  </link>

  <!-- Joint/Link - back caster -->
  <joint name="joint_back_caster" type="fixed">
    <origin rpy="0 0 0" xyz="-0.1 0.0 -0.15"/>
    <child link="back_caster"/>
    <parent link="base_link"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="0.0"/>
  </joint>
  <link name="back_caster">
    <inertial>
      <mass value="2"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.00025" ixy="0" ixz="0" iyy="0.00025" iyz="0" izz="0.00025"/>
    </inertial>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.025"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.025"/>
      </geometry>
      <material name="Blue">
        <color rgba="0.1 0.1 0.4 1"/>
      </material>
    </visual>
  </link>

  <!-- Laser scan -->
  <joint name="joint_laser_scan" type="fixed">
    <origin rpy="3.14 0 0" xyz="0.25 0 0.075"/>
    <child link="front_laser"/>
    <parent link="base_link"/>
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>
  <link name="front_laser">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="0.5"/>
      <inertia ixx="0.00080625" ixy="0" ixz="0" iyy="0.00080625" iyz="0" izz="0.0028125"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.02" radius="0.03"/>
      </geometry>
      <material name="Red">
        <color rgba="0.4 0.1 0.1 1"/>
      </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.02" radius="0.03"/>
      </geometry>
    </collision>
  </link>


  <gazebo reference="base_link">
      <material>Gazebo/Black</material>
  </gazebo>
  <gazebo reference="front_caster">
      <material>Gazebo/Blue</material>
      <mu1>0</mu1>
      <mu2>0</mu2>
  </gazebo>
  <gazebo reference="back_caster">
      <material>Gazebo/Blue</material>
      <mu1>0</mu1>
      <mu2>0</mu2>
  </gazebo>
  <gazebo reference="left_wheel">
      <material>Gazebo/Blue</material>
      <mu1>1</mu1>
      <mu2>1</mu2>
  </gazebo>
  <gazebo reference="right_wheel">
      <material>Gazebo/Blue</material>
      <mu1>1</mu1>
      <mu2>1</mu2>
  </gazebo>
  <gazebo reference="front_laser">
      <material>Gazebo/Red</material>
  </gazebo>
</robot>

